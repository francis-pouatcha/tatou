   <inherits name="org.jboss.errai.jpa.JPA" /> --> 
    <!--  <inherits name="org.jboss.errai.jpa.sync.DataSync" />  --> 
	<inherits name="com.google.gwt.validation.Validation" />
	<inherits name='org.hibernate.validator.HibernateValidator' />
	<inherits name="com.google.gwt.logging.Logging"/>
	<inherits name='com.google.gwt.query.Query'/>
	<!-- <inherits name='com.xedge.jquery.GWTJQuery'/>
	<set-property name="user.agent" value="ie6,gecko1_8" /> -->
	<set-property name="gwt.logging.logLevel" value="FINE"/>
    <set-property name="gwt.logging.popupHandler" value="DISABLED"/>
	<source path='' />

	<replace-with class="cm.adorsys.gpao.support.Erraihtml5ValidatorFactory">
		<when-type-is class="javax.validation.ValidatorFactory" />
	</replace-with>
	
	<inherits name="com.google.gwt.logging.Logging"/>
	<replace-with
		class="cm.adorsys.gpao.support.Erraihtml5ValidationMessageResolver">
		<when-type-is
			class="com.google.gwt.validation.client.ProviderValidationMessageResolver" />
	</replace-with>
      
